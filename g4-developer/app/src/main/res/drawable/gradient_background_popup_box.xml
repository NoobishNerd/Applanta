<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <solid android:color="#ffffff"></solid>
        </shape>
    </item>
    <item android:left="1dp"
        android:top="2dp"
        android:right="1dp">
        <shape>
            <gradient
                android:angle="-90"
                android:startColor="#A7C957"
                android:centerColor="#7FAA51"
                android:endColor="#386641"
                android:type="linear" />
        </shape>
    </item>
</layer-list>